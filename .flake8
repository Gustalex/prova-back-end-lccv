[flake8]
max-line-length = 120
extend-ignore = E203, W503, F401, D100, D101, D104, D200
exclude =
    .git,
    __pycache__,
    docs/source/conf.py,
    old,
    build,
    dist,
    *migrations*,
    venv,
    .venv,
    env,
    .env,
    .eggs,
    *.egg,
    .tox,
    .nox,
    .pytest_cache,
    .mypy_cache,
    .hypothesis,
